      <Damage disconnect="2500" explode="2500" explosionPower="5" explosionSize="10" />
      <AttachPoints>
         <AttachPoint location="TopCenter" fuelLine="true" />
         <AttachPoint location="BottomCenter" fuelLine="true" />
